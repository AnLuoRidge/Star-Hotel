<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane xmlns:fx="http://javafx.com/fxml/1" alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8"
          fx:controller="main.mainViewController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <BorderPane prefHeight="300.0" prefWidth="400.0">
         <top>
            <Label text="Star Hotel Management System" BorderPane.alignment="TOP_CENTER">
               <BorderPane.margin>
                  <Insets top="1.0"/>
               </BorderPane.margin>
            </Label>
         </top>
         <center>
            <Button fx:id="customerButton" mnemonicParsing="false" text="Customer Panel" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets bottom="10.0"/>
               </BorderPane.margin>
            </Button>
         </center>
      </BorderPane>
   </children>
</GridPane>
